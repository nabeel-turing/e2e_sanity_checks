                  "textRun": {
                    "content": "Customer Service Standards",
                    "style": {
                      "fontFamily": "Calibri",
                      "fontSize": {
                        "magnitude": 12,
                        "unit": "PT"
                      }
                    }
                  }
                }
              ]
