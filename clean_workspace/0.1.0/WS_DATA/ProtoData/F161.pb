  "mimeType": "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
  "createdTime": "2025-07-21T18:05:28Z",
  "modifiedTime": "2025-05-01T19:35:19Z",
  "trashed": false,
  "starred": false,
  "parents": [],
  "owners": [
