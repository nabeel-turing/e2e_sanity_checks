    },
    {
      "elementId": "p53",
      "text": "www.sec.gov"
    },
    {
      "elementId": "p54",
      "text": "2. www.sec.gov"
    },
    {
      "elementId": "p55",
      "text": "www.sec.gov"
    },
    {
      "elementId": "p56",
      "text": "16. Amendments Any amendments or modifications to this SOW must be in writing and signed by both parties."
    },
    {
      "elementId": "p57",
      "text": "17. Acceptance By signing below, both parties agree to the terms and conditions set forth in this Statement of Work."
    },
    {
      "elementId": "p58",
      "text": "DESIGNER: Alex Sterling ELV Designs Date: May 2, 2025"
    },
    {
      "elementId": "p59",
